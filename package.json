{
  "name": "@dahlia-labs/numoen-config",
  "version": "0.4.1",
  "description": "Configuration for the Numoen app",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/numoen/config.git"
  },
  "keywords": [
    "celo",
    "blockchain",
    "mobius"
  ],
  "author": "Kyle Scott <kyle@numoen.com>",
  "license": "AGPL-3.0",
  "packageManager": "yarn@3.3.0",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@saberhq/eslint-config": "^2.1.0",
    "@saberhq/tsconfig": "^2.1.0",
    "@types/eslint": "^8.4.10",
    "@types/lodash": "^4.14.190",
    "@types/node": "18.11.9",
    "@types/prettier": "^2.7.1",
    "@yarnpkg/doctor": "^4.0.0-rc.31",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "ethers": "^5.7.2",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "ts-node": "10.9.1",
    "tslib": "^2.4.1",
    "typedoc": "^0.23.21",
    "typescript": "4.7.3"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@dahlia-labs/numoen-utils": "^0.10.1",
    "@dahlia-labs/token-utils": "^0.10.1",
    "@dahlia-labs/uniswapv2-utils": "^0.10.1",
    "@dahlia-labs/use-ethers": "^0.10.1",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "jsbi": "^4.3.0",
    "lodash": "^4.17.21",
    "tiny-invariant": "^1.3.1"
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.cjs.json",
    "clean": "rm -fr dist/",
    "typecheck": "tsc --project tsconfig.cjs.json",
    "lint": "eslint . --cache",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "prepare": "husky install",
    "doctor:packages": "yarn doctor packages/"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json,js,yml,yaml}": "prettier --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/numoen/config#readme"
}
